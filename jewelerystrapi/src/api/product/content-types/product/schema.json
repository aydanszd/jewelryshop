{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "biginteger"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "mappedBy": "products"
    },
    "product_colors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-color.product-color",
      "mappedBy": "products"
    },
    "product_sizes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-size.product-size",
      "mappedBy": "products"
    },
    "product_stocks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-stock.product-stock",
      "mappedBy": "products"
    }
  }
}
